{
  "name": "应用名称",
  "appId": "your-app-name",
  "description": "应用描述",
  "manifestVersion": "v0",
  "stateSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "app_state": {
        "type": "object",
        "properties": {
          "projectId": {
            "type": "string",
            "description": "项目ID，用于数据库保存"
          }
        },
        "required": ["projectId"],
        "additionalProperties": false
      },
      "content": {
        "type": "object",
        "description": "内容数据，具体格式由 Prompt 指导",
        "additionalProperties": true
      }
    },
    "required": ["app_state", "content"],
    "additionalProperties": false
  },
  "stateSchemaVersion": "0.0.0"
}
