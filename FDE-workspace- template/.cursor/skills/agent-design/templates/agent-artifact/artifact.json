{
  "name": "my-agent",
  "type": "agent",
  "version": "0.1.0",
  "description": "Agent 的简短描述",
  "specification": {
    "agent_spec": {
      "exported_agents": [
        {
          "name": "my-agent",
          "version": "0.1.0",
          "yaml_path": "./src/my_agent/my_agent.yaml"
        }
      ],
      "tools": [
        {
          "name": "my-tool-base",
          "version": "0.1.0",
          "imported_tools": [
            "TodoWrite",
            "Finish"
          ]
        },
        {
          "name": "my-tool-search",
          "version": "0.1.0",
          "imported_tools": [
            "Search"
          ]
        },
        {
          "name": "my-tool-filesystem",
          "version": "0.1.0",
          "imported_tools": [
            "Write",
            "Read",
            "Edit",
            "LS"
          ]
        }
      ],
      "sub_agents": [
        {
          "name": "my-sub-agent",
          "version": "0.1.0"
        }
      ],
      "mcp_servers": [
        {
          "name": "external-api",
          "type": "http",
          "url": "https://api.example.com/mcp"
        }
      ]
    }
  }
}
